{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "core:window:allow-center",
    "core:window:allow-is-resizable",
    "shell:allow-open",
    "fs:write-all",
    "fs:write-files",
    "fs:read-all",
    "fs:read-files",
    "fs:scope-applocaldata-recursive"
  ]
}